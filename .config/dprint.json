{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "incremental": true,
  "typescript": {
    "indentWidth": 2
  },
  "json": {},
  "markdown": {},
  "toml": {},
  "dockerfile": {},
  "includes": ["**/*.{ts,tsx,js,jsx,json,md,toml,dockerfile}", "dockerfile"],
  "excludes": ["**/node_modules", "**/*-lock.json", "**/target"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.59.1.wasm",
    "https://plugins.dprint.dev/json-0.13.2.wasm",
    "https://plugins.dprint.dev/markdown-0.11.2.wasm",
    "https://plugins.dprint.dev/toml-0.5.3.wasm",
    "https://plugins.dprint.dev/dockerfile-0.2.0.wasm"
  ]
}
