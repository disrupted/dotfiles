{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "incremental": true,
  "typescript": {
    "indentWidth": 2
  },
  "json": {
  },
  "markdown": {
  },
  "cargo_toml": {
  },
  "includes": ["**/*.{ts,tsx,js,jsx,json,md}", "Cargo.toml"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "**/target"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.48.0.wasm",
    "https://plugins.dprint.dev/json-0.12.1.wasm",
    "https://plugins.dprint.dev/markdown-0.9.2.wasm",
    "https://github.com/thomaseizinger/dprint-plugin-cargo-toml/releases/download/0.1.0/cargo-toml-0.1.0.wasm"
  ]
}
